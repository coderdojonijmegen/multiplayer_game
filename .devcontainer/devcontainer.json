{
  "name": "Haskell Server Dev Container",
  "build": {
    "dockerfile": "../engine/engine.Dockerfile",
    "target": "dev"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "haskell.haskell",
        "justus-w.haskell-ghci-dap",
        "sanaajani.hie-vscode",
        "mrmoore.vscode-integrated-haskell-shell",
        "vstirbu.vscode-mermaid-preview"
      ]
    }
  },
  "forwardPorts": [5000],
  "portsAttributes": {
    "5000": {
      "label": "Haskell Server"
    }
  },
  "remoteUser": "vscode"
}